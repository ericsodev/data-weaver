{
  "name": "svelte",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "seed:make": "knex --knexfile ./knex/knexfile.ts seed:make -x ts",
    "seed:run": "knex --knexfile ./knex/knexfile.ts seed:run",
    "migrate:make": "knex --knexfile ./knex/knexfile.ts migrate:make -x ts",
    "migrate:up": "knex --knexfile ./knex/knexfile.ts migrate:up",
    "migrate:down": "knex --knexfile ./knex/knexfile.ts migrate:down",
    "migrate:rollback": "knex --knexfile ./knex/knexfile.ts migrate:rollback",
    "migrate:latest": "knex --knexfile ./knex/knexfile.ts migrate:latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@types/js-cookie": "^3.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.17",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-superforms": "^2.6.2",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "bits-ui": "^0.18.6",
    "clsx": "^2.1.0",
    "formsnap": "^0.5.1",
    "js-cookie": "^3.0.5",
    "js-sha256": "^0.11.0",
    "knex": "^3.1.0",
    "lucide-svelte": "^0.343.0",
    "objection": "^3.1.4",
    "pg": "^8.11.5",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "zod": "^3.22.4"
  }
}
