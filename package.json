{
  "name": "svelte",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noemit",
    "format": "prettier --write .",
    "seed:make": "node --loader ts-node/esm ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts seed:make -x ts",
    "seed:run": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts seed:run",
    "migrate:make": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:make -x ts",
    "migrate:up": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:up",
    "migrate:down": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:down",
    "migrate:rollback": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:rollback",
    "migrate:latest": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.10",
    "@types/js-cookie": "^3.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.14.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "postcss": "^8.4.39",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^5.0.0-next.200",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^6.0.2",
    "sveltekit-superforms": "^2.16.1",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "typescript-svelte-plugin": "^0.3.39",
    "vite": "^5.3.4"
  },
  "dependencies": {
    "bits-ui": "^0.21.12",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.12",
    "formsnap": "^1.0.1",
    "js-cookie": "^3.0.5",
    "js-sha256": "^0.11.0",
    "knex": "^3.1.0",
    "lucide-svelte": "^0.412.0",
    "objection": "^3.1.4",
    "pg": "^8.12.0",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
    "type-fest": "^4.22.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  }
}
