{
  "name": "svelte",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noemit",
    "format": "prettier --write .",
    "seed:make": "node --loader ts-node/esm ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts seed:make -x ts",
    "seed:run": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts seed:run",
    "migrate:make": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:make -x ts",
    "migrate:up": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:up",
    "migrate:down": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:down",
    "migrate:rollback": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:rollback",
    "migrate:latest": "node --loader ts-node/esm --experimental-specifier-resolution=node ./node_modules/.bin/knex --knexfile ./knex/knexfile.ts migrate:latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@types/js-cookie": "^3.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.12.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.41.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^5.0.0-next.141",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-superforms": "^2.6.2",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.1",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.8.0",
    "typescript-svelte-plugin": "^0.3.39",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "bits-ui": "^0.21.12",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.11",
    "formsnap": "^1.0.1",
    "js-cookie": "^3.0.5",
    "js-sha256": "^0.11.0",
    "knex": "^3.1.0",
    "lucide-svelte": "^0.343.0",
    "objection": "^3.1.4",
    "pg": "^8.11.5",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "type-fest": "^4.18.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
